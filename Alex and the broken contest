#include<stdio.h>
#include<string.h>
int main()
{
	int i,j,n,m,f=0,o,p,q,r,cnt=0;
	char a[100],b[100]={"Danil"},c[100]={"Olya"},d[100]={"Slava"},e[100]={"Ann"},g[100]={"Nikita"};
	scanf("%s",a);
	n=strlen(a);
	m=strlen(b);
	o=strlen(c);
	p=strlen(d);
	q=strlen(e);
	r=strlen(g);
	if(n>=m)
	{
		for(i=0;i<n;i++)
		{
			if(a[i]==b[0])
			{
				for(j=i;j<m+i;j++)
				{
					if(a[j]!=b[j-i])
					{
						f=0;
						break;
					}
					else
					{
						f=1;
					}
				}
				if(f==1)
				{
					cnt++;
					
				}
			}
		}
	}
	if(n>=o)
	{
		for(i=0;i<n;i++)
		{		
			if(a[i]==c[0])
			{
				for(j=i;j<o+i;j++)
				{
					if(a[j]!=c[j-i])
					{
						f=0;
						break;
					}
					else
					{
						f=2;
					}
				}
				if(f==2)
				{
					cnt++;
					
				}
			}
		}
	}
	if(n>=p)
	{
		for(i=0;i<n;i++)
		{			
			if(a[i]==d[0])
			{
				for(j=i;j<p+i;j++)
				{
					if(a[j]!=d[j-i])
					{
						f=0;
						break;
					}
					else
					{
						f=3;
					}
				}
				if(f==3)
				{
					cnt++;
					
				}
			}
		}
	}
	if(n>=q)
	{
		for(i=0;i<n;i++)
		{		
			if(a[i]==e[0])
			{
				for(j=i;j<q+i;j++)
				{
					if(a[j]!=e[j-i])
					{
						f=0;
						break;
					}
					else
					{
						f=4;
					}
				}
				if(f==4)
				{
					cnt++;
					
				}
			}
		}
	}
	if(n>=r)
	{
		for(i=0;i<n;i++)
		{				
			
			if(a[i]==g[0])
			{
				for(j=i;j<r+i;j++)
				{
					if(a[j]!=g[j-i])
					{
						f=0;
						break;
					}
					else
					{
						f=5;
					}
				}
				if(f==5)
				{
					cnt++;
					
				}
			}
		}
	}
	if(cnt==1)
	{
		printf("YES");
	}
	else
	{
		printf("NO");
	}
}
